{
    "extends": ["standard", "plugin:react/recommended"],
    "plugins": [
        "standard",
        "promise",
        "react"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "node": true,
        "mocha": true,
        "browser": true
    },
    "rules": {
        "quotes": "off",
        "react/prop-types": "off",
        "react/jsx-indent": 1
    }
}